    "path": "latest",
    "react": "16.11.0",
    "react-docgen-typescript": "^1.16.2",
    "react-dom": "16.11.0",
    "react-scripts": "^3.4.0",
    "react-styleguidist": "^10.6.2",
    "source-map-support": "0.5.16",
    "start-server-and-test": "1.10.6",
    "style-loader": "^1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "3.7.2",
    "webpack": "^4.44.2"
  },
  "main": "dist/index.js",
